    "  rowwise() %>%\n",
    "  mutate(summed_tags = sum(as.numeric(c_across(everything())))) %>%\n",
    "  ungroup()\n",
    "\n",
    "# Move Tags Data to long format\n",
    "long_sum_trim_tag <- sum_trim_tag %>% \n",
    "  select(-appid) %>%\n",
    "  pivot_longer(cols = -summed_tags, names_to = \"names\", values_to = \"values\")\n",
    "\n",
    "ggplot(long_sum_trim_tag %>% arrange(desc(values)) %>% top_n(10), aes(x = names, y = values)) +\n",
    "  geom_col() +\n",
    "  coord_flip()"
   ]
  }
