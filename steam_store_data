   ],
   "source": [
    "#creating new table from PC requirements\n",
    "trim_required <- data.frame(trim_require)\n",
    "# Function to extract requirements\n",
    "extract_requirements <- function(text) {\n",
    "  # Remove unnecessary characters and split by <br/>\n",
    "  text <- gsub(\"\\\\r\\\\n\\\\t\", \"\", text)  # Clean up whitespace  #not catching all instances\n",
    "  text <- gsub(\"<.*?>\", \"\", text)      # Remove HTML tags\n",
    "  parts <- str_split(text, \"</p>\")[[1]]\n",
    "\n",
    "  minimum <- NA\n",
    "  recommended <- NA\n",
    "  #find minimum values\n",
    "  for (part in parts) {\n",
    "    min_match <- str_match(text, \"Minimum: (.+?)(?=(Recommended:|$))\") # Stop at \"Recommended:\" or end of string\n",
    "    if (!is.na(min_match[1, 2])) {\n",
    "      minimum <- min_match[1, 2]\n",
    "    }\n",
    "    #find recommended values\n",
    "    rec_match <- str_match(part, \"Recommended: (.+)\")  # Look for \"Recommended:\" anywhere\n",
    "    if (!is.na(rec_match[1, 2])) {\n",
    "      recommended <- rec_match[1, 2]\n",
    "    }\n",
    "  }\n",
    "\n",
    "  return(data.frame(Minimum = minimum, Recommended = recommended))\n",
    "}\n",
    "#apply above function, but keep the IDs!\n",
    "extracted_data <- apply(trim_required, 1, function(row) {\n",
    "  extracted <- extract_requirements(row[\"pc_requirements\"])\n",
    "  # Add the ID to the extracted data frame\n",
    "  extracted$id <- row[\"steam_appid\"]\n",
    "  return(extracted)\n",
    "})\n",
    "# Combine the list of data frames into a single data frame\n",
    "final_data <- do.call(rbind, extracted_data)\n",
    "# Reorder columns to put 'id' first\n",
    "final_data <- final_data[, c(\"id\", names(final_data)[names(final_data) != \"id\"])]\n",
    "#head(final_data)\n",
    "# Further processing (splitting into individual components)\n",
    "split_requirements <- function(req_string) {\n",
    "  if (is.na(req_string)) {\n",
    "    return(data.frame(Processor = NA, RAM = NA, VideoCard = NA, OS = NA, Input1 = NA, Input2 = NA, Connection = NA)) # Handle NAs\n",
    "  }\n",
    "  parts <- str_split(req_string, \", \")[[1]]\n",
    "  \n",
    "  extract_value <- function(part, pattern) {\n",
    "      match <- str_match(part, pattern)\n",
    "      ifelse(!is.na(match[1,2]), match[1,2], NA)\n",
    "  }\n",
    "\n",
    "  processor <- extract_value(parts[1], \"(.+) processor\")\n",
    "  ram <- extract_value(parts[2], \"(.+) ram\")\n",
    "  video_card <- extract_value(parts[3], \"(.+) video card\")\n",
    "  os <- extract_value(parts[4], \"(.+)\") # OS is the 4th item\n",
    "  input1 <- parts[5]\n",
    "  input2 <- parts[6]\n",
    "  connection <- parts[7]\n",
    "\n",
    "  return(data.frame(Processor = processor, RAM = ram, VideoCard = video_card, OS = os, Input1 = input1, Input2 = input2, Connection = connection))\n",
    "}\n",
    "#apply above function\n",
    "final_split_data <- data.frame()\n",
    "for(i in 1:nrow(final_data)){\n",
    "  min_data <- split_requirements(final_data$Minimum[i])\n",
    "  rec_data <- split_requirements(final_data$Recommended[i])\n",
    "  \n",
    "  combined_row <- cbind(final_data$id[i], min_data, rec_data) # Add ID here\n",
    "  colnames(combined_row) <- c(\"id\", paste0(\"Min_\", colnames(min_data)), paste0(\"Rec_\", colnames(rec_data)))\n",
    "  final_split_data <- rbind(final_split_data, combined_row)\n",
    "}\n",
    "head(final_split_data)"
   ]
  },
