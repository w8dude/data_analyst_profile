    "#sleep date format fixed\n",
    "sleep$Date <- mdy_hms(sleep$SleepDay) %>% format(\"%m/%d/%Y\")\n",
    "head(sleep)\n",
    "#weight date format fixed\n",
    "weight$Date <- mdy_hms(weight$Date) %>% format(\"%m/%d/%Y\")\n",
    "head(weight)\n",
